From 5b57f665f073785a7d4d2e9c0c177d00de48237d Mon Sep 17 00:00:00 2001
From: Danil Zagoskin <z@gosk.in>
Date: Sat, 30 Nov 2024 18:37:51 +0000
Subject: [PATCH 42/43] arm64: dts: amlogic: odroid-go ultra: unify volume
 button labels

---
 arch/arm64/boot/dts/amlogic/meson-g12b-odroid-go-ultra.dts | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/amlogic/meson-g12b-odroid-go-ultra.dts b/arch/arm64/boot/dts/amlogic/meson-g12b-odroid-go-ultra.dts
index 4accbcda7e4b..67fa8e2ff6f2 100644
--- a/arch/arm64/boot/dts/amlogic/meson-g12b-odroid-go-ultra.dts
+++ b/arch/arm64/boot/dts/amlogic/meson-g12b-odroid-go-ultra.dts
@@ -58,12 +58,12 @@ gpio-keys {
 		autorepeat;
 
 		volume-up-button {
-			label = "VOLUME-UP";
+			label = "VOLUMEUP";
 			linux,code = <KEY_VOLUMEUP>;
 			gpios = <&gpio GPIOX_8 GPIO_ACTIVE_LOW>;
 		};
 		volume-down-button {
-			label = "VOLUME-DOWN";
+			label = "VOLUMEDOWN";
 			linux,code = <KEY_VOLUMEDOWN>;
 			gpios = <&gpio GPIOX_9 GPIO_ACTIVE_LOW>;
 		};
-- 
